{
  "name": "memo",
  "type": "module",
  "packageManager": "pnpm@9.1.3",
  "scripts": {
    "build": "vitepress build",
    "dev": "vitepress dev --open --port 3333",
    "lint": "eslint .",
    "prepare": "simple-git-hooks",
    "prettier": "prettier --write .",
    "preview": "vitepress preview",
    "up": "taze latest -f -I"
  },
  "dependencies": {
    "@ayingott/sucrose": "^0.0.37"
  },
  "devDependencies": {
    "@ayingott/eslint-config": "^1.4.0",
    "@ayingott/prettier-config": "^1.2.0",
    "@iconify-json/carbon": "^1.1.34",
    "@types/node": "^20.12.12",
    "@vue-macros/volar": "^0.21.1",
    "eslint": "^9.3.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.2.5",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.13.8",
    "typescript": "^5.4.5",
    "unocss": "^0.60.3",
    "unplugin-vue-macros": "^2.9.3",
    "vite-plugin-vue-devtools": "^7.2.1",
    "vitepress": "^1.2.2",
    "vue": "^3.4.27"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "prettier": "@ayingott/prettier-config"
}
