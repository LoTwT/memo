{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "baseUrl": ".",
    "module": "esnext",
    "moduleResolution": "node",
    "paths": {},
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "include": ["**/*"],
  "exclude": ["node_modules"],
}
